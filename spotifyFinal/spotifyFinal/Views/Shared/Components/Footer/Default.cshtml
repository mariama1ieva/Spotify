
@using Service.ViewModels.Setting
 @model List<SettingListVM>




<section id="footer">
    <div class="content">
        <div class="all-footer">
            <div class="footer-info">
                <div class="companies">
                    <h3>Company</h3>
                    <a href="https://www.spotify.com/az-az/about-us/contact/">About</a>
                    <a href="https://www.lifeatspotify.com/">Jobs</a>
                    <a href="https://newsroom.spotify.com/">For the Record</a>
                </div>
                <div class="communities">
                    <h3>Communities</h3>
                    <a href="https://artists.spotify.com/home">For Artist</a>
                    <a href="https://developer.spotify.com/">Developers</a>
                    <a href="https://ads.spotify.com/en-US/">Advertising</a>
                    <a href="https://investors.spotify.com/home/default.aspx">Inverstors</a>
                </div>
                <div class="useful">
                    <h3>Contact Info</h3>
                   @foreach (var item in Model)
                    {
                        <div class="mail">
                            <span>

                                 @if (item.Key == "Email" || item.Key == "Address" || item.Key == "Number")
                                {
                                    <span>@item.Value</span>
                                }
                            </span>
                        </div>
                    }
                 
                </div>
                <div class="socialMedia-icons">
                    <button><a  href="https://www.instagram.com/spotify/" ><i class="fa-brands fa-instagram"></i></a></button>
                    <button><a href="https://x.com/spotify"><i class="fa-brands fa-twitter"></i></a></button>
                    <button><a href="https://www.facebook.com/Spotify"><i class="fa-brands fa-facebook"></i></a></button>
                </div>
            </div>
            <div class="footer-divide">
                <div class="dvide">
                    <p>© 2024 Spotify AB</p>
                </div>
            </div>
        </div>
    </div>
</section>
        </section>

    </div>
<script src="https://kit.fontawesome.com/680cf1a7ac.js" crossorigin="anonymous"></script>

